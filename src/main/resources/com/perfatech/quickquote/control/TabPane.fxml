<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
    fx:id="tabAreaAnchorPane"
    fx:controller="com.perfatech.quickquote.control.TabPane">

    <VBox fx:id="rentalUnitPickerArea" prefHeight="100.0" prefWidth="550.0">
        <children>
            <HBox id="HBox" alignment="CENTER">
                <children>
                    <Label id="" alignment="CENTER" prefWidth="71.0" text="Property:" />
                    <ComboBox fx:id="unitComboBox" prefHeight="25.0" prefWidth="150.0" />
                </children>
            </HBox>
        </children>
        <padding>
            <Insets top="10.0" />
        </padding>
    </VBox>

</AnchorPane>